<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>currentDate</managed-bean-name>
  <managed-bean-class>java.util.Date</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/admin/code/view.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>viewCodeDetail</from-outcome>
   <to-view-id>/admin/code/viewCodeDetail.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>EditCode</from-outcome>
   <to-view-id>/admin/code/EditCode.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>editSelect</from-outcome>
   <to-view-id>/admin/code/edit-liststudent-sel.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/code/EditCode.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>view</from-outcome>
   <to-view-id>/admin/code/view.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/code/viewCodeDetail.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>view</from-outcome>
   <to-view-id>/admin/code/view.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>addStudent</from-outcome>
   <to-view-id>/admin/code/addStudent.xhtml</to-view-id>
   <redirect></redirect>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/subject/view.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>edit</from-outcome>
   <to-view-id>/admin/subject/edit.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>edit-teachers</from-outcome>
   <to-view-id>/admin/subject/edit-teachers.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>view-detail</from-outcome>
   <to-view-id>/admin/subject/view-detail.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>dashboard</from-outcome>
   <to-view-id>/admin/dashboard.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/welcome.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>manager</from-outcome>
   <to-view-id>/admin/dashboard.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>student</from-outcome>
   <to-view-id>/student/student.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>teacher</from-outcome>
   <to-view-id>/teacher/teacher.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/dashboard.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>welcome</from-outcome>
   <to-view-id>/welcome.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/admin/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <action-listener>org.primefaces.application.DialogActionListener</action-listener>
  <navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
  <view-handler>org.primefaces.application.DialogViewHandler</view-handler>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
